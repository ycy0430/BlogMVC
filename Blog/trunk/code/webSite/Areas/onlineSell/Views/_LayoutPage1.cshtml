@{
    var currentUser = ViewBag.CurrentUser as web.Models.User;
    var name = currentUser == null ? string.Empty : currentUser.Name;
    var config = ViewBag.Config as web.ViewsModels.BlogConfig;
    var dbContext = CustomHtmlHelper.GetDbContext();
    var listCatalog = dbContext.Catalogs.ToList();
    var listBlog = dbContext.Blogs.ToList();

}
<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@config.Name - 个人博客</title>
    <link type="text/css" rel="stylesheet" href="/include/index_files/blog-common.css">
    <link id="MainCss" type="text/css" rel="stylesheet" href="/include/index_files/bundle-CodingLife.css">
    <link type="text/css" rel="stylesheet" href="/include/index_files/128442.css">
    <link type="text/css" rel="stylesheet" href="/Content/css/ycy.css">
    <link id="mobile-style" media="only screen and (max-width: 768px)" type="text/css" rel="stylesheet" href="/include/index_files/bundle-CodingLife-mobile.css">

    <script src="/include/index_files/analytics.js" async=""></script>
    <script src="/include/index_files/jquery.js" type="text/javascript"></script>
    <script type="text/javascript">var currentBlogApp = 'ycy0430', cb_enable_mathjax = false; var isLogined = false;</script>
    <script src="/include/index_files/blog-common.js" type="text/javascript"></script>



    <link rel="stylesheet" type="text/css" href="/include/assets/waifu.css" />
    <script src="https://cdn.bootcss.com/jquery/3.2.1/jquery.min.js"></script>


</head>
<body>


    <!--done-->
    <div id="home">
        <div id="header">
            <div id="blogTitle">

                <!--done-->
                <h1><a id="Header1_HeaderTitle" class="headermaintitle" href="/onlineSell/Home/Index">@config.Name</a></h1>
                <h2>@config.Sign</h2>



            </div><!--end: blogTitle 博客的标题和副标题 -->
            <div id="navigator">

                <ul id="navList">
                    <li><a id="blog_nav_myhome" class="menu" href="/onlineSell/Home/Index">首页</a></li>
                    @foreach (var item in listCatalog)
                    {
                        <li><a id="blog_nav_myhome" class="menu" href="@Url.Action("index", "Home", new { catalogid = item.Id })">@item.Name</a></li>
                    }


                    <li class="li-r"><a id="blog_nav_admin" class="menu" rel="nofollow" href="/onlineSell/Home/about">关于</a></li>
                    <li class="li-r"><a id="blog_nav_admin" class="menu" rel="nofollow" href="/" target="_blank">管理</a></li>
                    
                </ul>

            </div><!--end: navigator 博客导航栏 -->
        </div><!--end: header 头部 -->
        <div id="main">
            <div id="mainContent">
                <div class="forFlow">
                    @RenderBody()
                </div><!--end: forFlow -->
            </div><!--end: mainContent 主体内容容器-->

            <div id="sideBar">
                <div id="sideBarMain">
                    @Html.Partial("~/Areas/onlineSell/Views/_SideBar.cshtml")
                </div><!--end: sideBarMain -->
            </div><!--end: sideBar 侧边栏容器 -->

        </div><!--end: main -->
        <div class="clear"></div>



        <div id="footer">
            <!--done-->
            <a href="https://github.com/ycy0430" target="_blank" style="color:#ccc">Copyright ©@DateTime.Now.Year ycy</a> 
        </div><!--end: footer -->
    </div><!--end: home 自定义的最大容器 -->
    <!--看板娘-->
    <div class="waifu">
        <div class="waifu-tips"></div>
        <canvas id="live2d" width="280" height="250" class="live2d"></canvas>
        <div class="waifu-tool">
            <span class="fui-home"></span>
            <span class="fui-chat"></span>
            @*<span class="fui-eye"></span>*@
            <span class="fui-user"></span>
            <span class="fui-photo"></span>
            <span class="fui-info-circle"></span>
            <span class="fui-cross"></span>
        </div>
    </div>

    <script src="/include/assets/waifu-tips.js"></script>
    <script src="/include/assets/live2d.js"></script>
    <script type="text/javascript">initModel("/include/assets/")</script>
    <!--播放器-->
    <script src="https://myhkw.cn/player/js/player.js" id="myhk" key="159674402112" m="1"></script>
</body>
</html>